package leetCodeAlgoEasy;

import java.util.Arrays;

public class missingNumberInArray {

	public static void main(String[] args) {
		int[] arr = {0,1,3};
		missingNumber(arr);

	}
    public static int missingNumber(int[] nums) {
    	Arrays.sort(nums);
    	int n = nums[nums.length-1];
    	System.out.println(n);
		return 0;
        
    }

}
