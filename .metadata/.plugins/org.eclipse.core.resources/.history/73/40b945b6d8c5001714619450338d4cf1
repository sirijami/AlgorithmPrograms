
public class Exam {

	public static void main(String[] args) {
		int[] arr = {1,2,3,5,7};
		isSumPair(arr);

	}

	private static void isSumPair(int[] arr) {
		int start = 0;
		int end = arr.length-1;
		for(int i = 0; i < arr.length; i++){
			int num = end - start;
			for(int j = start+1; j <= end-1; j++){
				if(num == arr[j]){
					System.out.println("Pairs exist");
				}else {
					end--;
				}
			}
		}
		
	}

}
