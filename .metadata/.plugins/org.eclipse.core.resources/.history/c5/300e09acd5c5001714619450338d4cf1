
public class Exam {

	public static void main(String[] args) {
		int[] arr = {1,2,3,5,7};
		isSumPair(arr);

	}

	private static void isSumPair(int[] arr) {
		int start = 0;
		int end = arr.length-1;
		while(start < end){
			int num = end-start;
			for(int i = start+1; i <= end-1; i++){
				if(num == arr[i]){
					System.out.println("Pairs exist");
					break;
				}else {
					end--;
				}
			}
		}

		
	}

}
