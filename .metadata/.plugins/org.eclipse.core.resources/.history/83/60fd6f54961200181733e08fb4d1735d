package SingleLinkedList;

public class SingleLinkedList {
	Node head;
	
	public void printList() {
		if(head == null){
			System.out.println("Emplty List");
			return ;
		}
		Node p = head;
		while(p.next != null){
			System.out.print(p.data + " -> ");
			p = p.next;
		}
		System.out.println(p.data + " -> null");		
	}
	
	public int countNodes(){
		int count = 0;
		if(head == null){
			System.out.println("Emplty List");
			return count;
		}
		Node p = head;
		while(p != null){
			count++;
			p = p.next;
		}
		return count;
	}

        

}
