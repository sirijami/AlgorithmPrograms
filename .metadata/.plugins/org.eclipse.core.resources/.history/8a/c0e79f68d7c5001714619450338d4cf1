
public class Tree {
	Node root;
	
	public Tree(){
		root = null;
	}

	public static void main(String[] args) {
		Tree t = new Tree();
		t.root = new Node(2);
        t.root.left = new Node(2);
        t.root.right = new Node(3);
        t.root.left.left = new Node(4);
        t.root.left.right = new Node(5);
        t.root.right.left = new Node(6);
        t.root.right.right = new Node(7);
        

	}

}
